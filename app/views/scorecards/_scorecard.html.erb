<div id="<%= dom_id(parent) %>">
  <table class="scorecard">
    <tr>
      <th class="align_left" colspan="12">Front 9</th>
    </tr>
    <%= render :partial => 'scorecards/holes', :locals => {:parent => parent,:holes => parent.front_nine} %>
    <tr>
      <th class="align_left" colspan="12">Back 9</th>
    </tr>
    <%= render :partial => 'scorecards/holes', :locals => {:parent => parent,:holes => parent.back_nine} %>
  </table>
</div>
